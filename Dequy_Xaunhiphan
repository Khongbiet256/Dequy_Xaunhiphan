/* Đệ quy tuyến tính */ 
#include<bits/stdc++.h>
using namespace std;

int n,X[100];
void inkq(){
	for(int i=1;i<=n;i++)
	{
	cout<<X[i];
	}
	cout<<endl;
}
void Try(int i)
{
	for(int j=1;j<=1;j++)
	{
		X[i]=j;
		if(i==n) inkq();
		else Try(i+1);
	}
	
} 
int main(){
	freeopen("input.txt","r",stdin);
	freeopen("output.txt","w",stdout);
	cin>>n;
	Try(1);
	return 0;

} 
